{
  "name": "figma-research",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "start:storybook": "npm run storybook",
    "transform:tokens": "npx token-transformer ./tokens/export-tokens.json ./tokens/transformed-tokens.json --expandTypography --expandShadow",
    "transform:tokens-multi-files-example": "npx token-transformer ./tokens/export-tokens.json tokens/global.json global && npx token-transformer ./tokens/export-tokens.json tokens/light.json global,light,theme global && npx token-transformer ./tokens/export-tokens.json tokens/dark.json global,dark,theme global",
    "build:tokens": "ts-node ./build.ts",
    "generate:tokens": "npm run transform:tokens && npm run build:tokens",
    "--- Token studio ---": "",
    "generate:tokens-via-sd-transforms": "ts-node ./build-via-sd-transforms.ts",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@vanilla-extract/css": "^1.13.0",
    "next": "13.5.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "token-transformer": "^0.0.33"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^7.5.2",
    "@storybook/addon-interactions": "^7.5.2",
    "@storybook/addon-links": "^7.5.2",
    "@storybook/addon-onboarding": "^1.0.8",
    "@storybook/addon-styling-webpack": "^0.0.5",
    "@storybook/blocks": "^7.5.2",
    "@storybook/nextjs": "^7.5.2",
    "@storybook/react": "^7.5.2",
    "@storybook/testing-library": "^0.2.2",
    "@tokens-studio/sd-transforms": "^0.11.6",
    "@types/node": "^20.8.6",
    "@types/react": "^18.2.28",
    "@types/react-dom": "^18.2.13",
    "@vanilla-extract/next-plugin": "2.3.1",
    "eslint": "^8.51.0",
    "eslint-config-next": "13.5.4",
    "eslint-plugin-storybook": "^0.6.15",
    "storybook": "^7.5.2",
    "style-dictionary": "^3.8.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  }
}
