{
  "name": "figma-research",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "start:dev": "npm run dev",
    "start:storybook": "npm run storybook",
    "transform:tokens": "npx token-transformer ./src/design-tokens/export-tokens.json ./src/design-tokens/design-tokens.json --expandTypography --expandShadow",
    "build:tokens": "ts-node ./scripts/design-tokens/build-design-tokens.ts  && npm run remove:transformed-tokens",
    "generate:tokens": "npm run transform:tokens && npm run build:tokens && npm run remove:transformed-tokens",
    "remove:transformed-tokens": "rm -rf ./src/design-tokens/design-tokens.json"
  },
  "dependencies": {
    "@vanilla-extract/css": "^1.13.0",
    "next": "13.5.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^7.5.2",
    "@storybook/addon-interactions": "^7.5.2",
    "@storybook/addon-links": "^7.5.2",
    "@storybook/addon-onboarding": "^1.0.8",
    "@storybook/addon-styling-webpack": "^0.0.5",
    "@storybook/blocks": "^7.5.2",
    "@storybook/nextjs": "^7.5.2",
    "@storybook/react": "^7.5.2",
    "@storybook/testing-library": "^0.2.2",
    "@types/node": "^20.8.6",
    "@types/react": "^18.2.28",
    "@types/react-dom": "^18.2.13",
    "@vanilla-extract/next-plugin": "2.3.1",
    "@vanilla-extract/webpack-plugin": "^2.3.1",
    "css-loader": "^6.8.1",
    "eslint": "^8.51.0",
    "eslint-config-next": "13.5.4",
    "eslint-plugin-storybook": "^0.6.15",
    "mini-css-extract-plugin": "^2.7.6",
    "storybook": "^7.5.2",
    "style-dictionary": "^3.8.0",
    "style-loader": "^3.3.3",
    "token-transformer": "^0.0.33",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  }
}
